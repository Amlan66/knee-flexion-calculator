{
    "manifest_version": 3,
    "name": "Knee Flexion Calculator",
    "version": "1.0",
    "description": "Calculate knee flexion angle from images",
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": ["storage"],
    "web_accessible_resources": [
      {
        "resources": ["lib/chart.js"],
        "matches": ["<all_urls>"]
      }
    ]
  }
